{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"Processes": {
			"type": "array",
			"items": [
				{
					"type": "object",
					"properties": {
						"ProcessName": {
							"type": "string"
						},
						"GracePeriod": {
							"type": "string"
						},
						"SubPrograms": {
							"type": "array",
							"items": [
								{
									"type": "object",
									"properties": {
										"ProgramPath": {
											"type": "string"
										},
										"KeepRunning": {
											"type": "boolean"
										},
										"Delay": {
											"type": "string"
										}
									}
								}
							]
						}
					}
				}
			]
		},
		"CheckInterval": {
			"type": "string"
		},
		"LogToConsole": {
			"type": "boolean"
		},
		"LogToFile": {
			"type": "boolean"
		}
	}
}